<div class="mz-productlist {% block module-classes %}{% endblock module-classes %} col-xs-12 no-pad">
    {% if model.items %}
    <div class="mz-productlist-list {% block list-classes %}{% endblock list-classes %}">
        {% for prod in model.items %}
            <div class="mz-productlist-item" data-mz-product="{{ prod.productCode }}">{% include "modules/product/product-listing" with model=prod %}</div>
        {% endfor %}
    </div>
    {% endif %}
</div>


