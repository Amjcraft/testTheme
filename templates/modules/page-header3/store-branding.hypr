{% if themeSettings.themeLogo %}
	<h1 class="mz-storebranding" id="store-branding">
	    <a href="/" title="{{ siteContext.generalSettings.websiteName }}">
	    	<img class="img-responsive" src="{{ themeSettings.themeLogo.imageUrl }}">
    	</a>
	</h1>
{% else %}
	<h1 class="mz-storebranding" id="store-branding">
	    <a href="/" title="{{ siteContext.generalSettings.websiteName }}">{{ siteContext.generalSettings.websiteName }}</a>
	</h1>
{% endif %}