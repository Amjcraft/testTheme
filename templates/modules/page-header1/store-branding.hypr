{% if themeSettings.themeLogo %}
	<h1 class="mz-storebranding" id="store-branding">
	    <a href="/" title="{{ siteContext.generalSettings.websiteName }}" rel="nofollow">
	    	<img class="img-responsive" src="{{ themeSettings.themeLogo.imageUrl }}">
    	</a>
	</h1>
{% else %}
	<h1 class="mz-storebranding" id="store-branding">
	    <a href="/" title="{{ siteContext.generalSettings.websiteName }}" rel="nofollow">{{ siteContext.generalSettings.websiteName }}</a>
	</h1>
{% endif %}